# Agent 기본 설정
AGENT_NAME=ss-usb-test-agent
AGENT_VERSION=0.1.0
AGENT_DEBUG=false

# USB Test 설정
AGENT_USB_TEST_PATH=C:/USB_Test/USB Test.exe
AGENT_HEALTH_REPORT_PATH=C:/USB_Test/Card_HealthReport.exe
AGENT_MAX_SLOTS=4

# Backend 연결 설정
AGENT_BACKEND_URL=http://localhost:8000
AGENT_BACKEND_WS_URL=ws://localhost:8000/api/agent/ws
AGENT_API_KEY=your-api-key-here

# 타임아웃 설정 (초)
AGENT_STATE_CHECK_INTERVAL=10
AGENT_STATE_CHECK_RETRIES=10
AGENT_HANG_THRESHOLD=300

# 알림 설정
AGENT_JANDI_WEBHOOK_URL=https://wh.jandi.com/connect-api/webhook/your-webhook-url
AGENT_NOTIFICATION_ENABLED=true

# 로깅 설정
LOG_LEVEL=INFO
LOG_FORMAT=json
